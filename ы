-- Godmode
PlayerTab:CreateToggle({
   Name = "Godmode",
   CurrentValue = false,
   Flag = "Godmode",
   Callback = function(Value)
        getgenv().enableGodmode = Value
        
        local godcon
        local function UpdateGod()
            if godcon then
                godcon:Disconnect()
                godcon = nil
            end
            
            if Humanoid and getgenv().enableGodmode then
                godcon = Humanoid.HealthChanged:Connect(function()
                    if Humanoid.Health < Humanoid.MaxHealth then
                        Humanoid.Health = Humanoid.MaxHealth
                    end
                end)
            end
        end
        
        if Value then
            Rayfield:Notify({
                Title = "Godmode",
                Content = "Enabled godmode",
                Duration = 2,
                Image = 4483362458
            })
            UpdateGod()
            
            LocalPlayer.CharacterAdded:Connect(function(newChar)
                Character = newChar
                Humanoid = Character:WaitForChildOfClass("Humanoid")
                UpdateGod()
            end)
        else
            if godcon then
                godcon:Disconnect()
                godcon = nil
            end
        end
    end
})

-- Fling Timeout
MainTab:CreateSlider({
   Name = "Fling Timeout",
   Range = {0.5, 10},
   Increment = 0.1,
   Suffix = "seconds",
   CurrentValue = 2.5,
   Flag = "FlingTimeout",
   Callback = function(Value)
        getgenv().timeout = Value
    end
})

